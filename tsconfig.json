{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": "./",
    "paths": {
      "@images/*": ["src/assets/images/*"],
      "@icons/*": ["src/assets/icons/*"],
      "@adapters/*": ["src/adapters/*"],
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@services/*": ["src/services/*"],
      "@utils/*": ["src/utils/*"],
    }
  }
}